<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LollyMaui.WebTextbooksWebPagePage"
             Title="WebTextbooks Web Pages"
             xmlns:common="clr-namespace:LollyCommon;assembly=LollyCommon"
             x:DataType="common:MWebTextbook">

    <ContentPage.ToolbarItems>
    </ContentPage.ToolbarItems>
    <Grid RowDefinitions="Auto,*">
        <Label Text="{Binding TEXTBOOKNAME}" />
        <WebView Grid.Row="1" x:Name="wbWebPage" />
    </Grid>
</ContentPage>
